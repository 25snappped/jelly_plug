<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Nullable>enable</Nullable>
    <LangVersion>latest</LangVersion>
    <EnableDynamicLoading>true</EnableDynamicLoading>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <RootNamespace>Jellyfin.Plugin.JellyPlug</RootNamespace>
    <AssemblyName>Jellyfin.Plugin.JellyPlug</AssemblyName>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Jellyfin.Common" Version="10.9.7" />
    <PackageReference Include="Jellyfin.Controller" Version="10.9.7" />
    <PackageReference Include="MediaBrowser.Model" Version="10.9.7" />
    <PackageReference Include="Microsoft.AspNetCore.Mvc.Core" Version="2.2.5" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Resources\**\*" />
  </ItemGroup>
</Project>
